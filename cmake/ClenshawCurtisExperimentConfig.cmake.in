# Use the following variables to compile and link against ClenshawCurtisExperiment:
#  CLENSHAWCURTISEXPERIMENT_FOUND          - True if ClenshawCurtisExperiment was found on your system
#  CLENSHAWCURTISEXPERIMENT_USE_FILE       - The file making ClenshawCurtisExperiment usable
#  CLENSHAWCURTISEXPERIMENT_DEFINITIONS    - Definitions needed to build with ClenshawCurtisExperiment
#  CLENSHAWCURTISEXPERIMENT_INCLUDE_DIRS   - List of directories where ClenshawCurtisExperiment' header file are
#  CLENSHAWCURTISEXPERIMENT_LIBRARY        - Library name
#  CLENSHAWCURTISEXPERIMENT_LIBRARIES      - List of libraries to link against
#  CLENSHAWCURTISEXPERIMENT_LIBRARY_DIRS   - List of directories containing ClenshawCurtisExperiment' libraries
#  CLENSHAWCURTISEXPERIMENT_ROOT_DIR       - The base directory of ClenshawCurtisExperiment
#  CLENSHAWCURTISEXPERIMENT_VERSION_STRING - A human-readable string containing the version
#  CLENSHAWCURTISEXPERIMENT_VERSION_MAJOR  - The major version of ClenshawCurtisExperiment
#  CLENSHAWCURTISEXPERIMENT_VERSION_MINOR  - The minor version of ClenshawCurtisExperiment
#  CLENSHAWCURTISEXPERIMENT_VERSION_PATCH  - The patch version of ClenshawCurtisExperiment

@PACKAGE_INIT@

set ( CLENSHAWCURTISEXPERIMENT_FOUND 1 )
set ( CLENSHAWCURTISEXPERIMENT_USE_FILE     "@PACKAGE_CLENSHAWCURTISEXPERIMENT_USE_FILE@" )

set ( CLENSHAWCURTISEXPERIMENT_DEFINITIONS  "@CLENSHAWCURTISEXPERIMENT_DEFINITIONS@" )
set_and_check (CLENSHAWCURTISEXPERIMENT_INCLUDE_DIR  "@PACKAGE_CLENSHAWCURTISEXPERIMENT_INCLUDE_DIR@")
set ( CLENSHAWCURTISEXPERIMENT_INCLUDE_DIRS "@PACKAGE_CLENSHAWCURTISEXPERIMENT_INCLUDE_DIRS@" )
set_and_check (CLENSHAWCURTISEXPERIMENT_LIBRARY      "@CLENSHAWCURTISEXPERIMENT_LIBRARY@")
set ( CLENSHAWCURTISEXPERIMENT_LIBRARIES    "@PACKAGE_CLENSHAWCURTISEXPERIMENT_LIBRARIES@" )
set ( CLENSHAWCURTISEXPERIMENT_LIBRARY_DIRS "@PACKAGE_CLENSHAWCURTISEXPERIMENT_LIBRARY_DIRS@" )
set_and_check (CLENSHAWCURTISEXPERIMENT_ROOT_DIR     "@PACKAGE_CLENSHAWCURTISEXPERIMENT_ROOT_DIR@")

set ( CLENSHAWCURTISEXPERIMENT_VERSION_STRING "@CLENSHAWCURTISEXPERIMENT_VERSION_STRING@" )
set ( CLENSHAWCURTISEXPERIMENT_VERSION_MAJOR  "@CLENSHAWCURTISEXPERIMENT_VERSION_MAJOR@" )
set ( CLENSHAWCURTISEXPERIMENT_VERSION_MINOR  "@CLENSHAWCURTISEXPERIMENT_VERSION_MINOR@" )
set ( CLENSHAWCURTISEXPERIMENT_VERSION_PATCH  "@CLENSHAWCURTISEXPERIMENT_VERSION_PATCH@" )

set (CLENSHAWCURTISEXPERIMENT_SWIG_INCLUDE_DIRS "@PACKAGE_CLENSHAWCURTISEXPERIMENT_SWIG_INCLUDE_DIRS@")
set (CLENSHAWCURTISEXPERIMENT_PYTHON@PYTHON_VERSION_MAJOR@_MODULE_PATH "@PACKAGE_PYTHON_MODULE_PATH@")

# Our library dependencies (contains definitions for IMPORTED targets)
include (${CMAKE_CURRENT_LIST_DIR}/ClenshawCurtisExperiment-Targets.cmake)
